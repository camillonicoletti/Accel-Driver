<?xml version="1.0"?>
<mavlink>
    <version>2</version>
    <enums>
        <enum name="MavTMList">
            <description>Enum list for all the telemetries that can be requested from ground.</description>
            <entry name="MAV_SYS_TM_ID" value="1">
                <description>State of system components</description>
            </entry>
            <entry name="MAV_FMM_TM_ID" value="2">
                <description>FMM status</description>
            </entry>
            <entry name="MAV_PIN_OBS_TM_ID" value="3">
                <description>Pin observer data</description>
            </entry>
            <entry name="MAV_LOGGER_TM_ID" value="4">
                <description>SD Logger stats</description>
            </entry>
            <entry name="MAV_TMTC_TM_ID" value="5">
                <description>TMTC status (mavlink + queue)</description>
            </entry>
            <entry name="MAV_TASK_STATS_TM_ID" value="6">
                <description>Task scheduler statistics</description>
            </entry>
            <entry name="MAV_DPL_TM_ID" value="7">
                <description>Deployment System Status</description>
            </entry>
            <entry name="MAV_ADA_TM_ID" value="8">
                <description>ADA Status</description>
            </entry>
            <entry name="MAV_ABK_TM_ID" value="9">
                <description>Aerobrakes data</description>
            </entry>
            <entry name="MAV_NAS_TM_ID" value="10">
                <description>NavigationSystem data</description>
            </entry>
            <!--<entry name="MAV_CAN_TM_ID" value="11">
                <description>Canbus stats</description>
            </entry>-->
            <entry name="MAV_ADC_TM_ID" value="12">
                <description>Analog Digital Converter data</description>
            </entry>
            <entry name="MAV_MS5803_TM_ID" value="13">
                <description>MS5803 barometer data</description>
            </entry>
            <entry name="MAV_BMX160_TM_ID" value="14">
                <description>BMX160 IMU data</description>
            </entry>
            <entry name="MAV_LIS3MDL_TM_ID" value="15">
                <description>LIS3MDL compass data</description>
            </entry>
            <entry name="MAV_GPS_TM_ID" value="16">
                <description>Piksi GPS data</description>
            </entry>
            <entry name="MAV_HR_TM_ID" value="18">
                <description>High Rate telemetry</description>
            </entry>
            <entry name="MAV_LR_TM_ID" value="19">
                <description>Low Rate telemetry</description>
            </entry>
            <entry name="MAV_TEST_TM_ID" value="20">
                <description>Test telemetry</description>
            </entry>
            <entry name="MAV_WINDTUNNEL_TM_ID" value="21">
                <description>Wind tunnel telemetry</description>
            </entry>
            <entry name="MAV_SENSORS_TM_ID" value="22">
                <description>Wind tunnel telemetry</description>
            </entry>
        </enum>
        <enum name="MavCommandList">
            <description>Enum of the commands that don't have any message payload.</description>
            <entry name="MAV_CMD_ARM" value="27">
                <description>Command to arm the rocket.</description>
            </entry>
            <entry name="MAV_CMD_DISARM" value="28">
                <description>Command to disarm the rocket.</description>
            </entry>
            <entry name="MAV_CMD_FORCE_LAUNCH" value="29">
                <description>Command to disarm the rocket.</description>
            </entry>
            <entry name="MAV_CMD_CALIBRATE_SENSORS" value="30">
                <description>Command to start sensor calibration.</description>
            </entry>
            <entry name="MAV_CMD_CALIBRATE_ALGOS" value="31">
                <description>Command to start algorithm calibration.</description>
            </entry>
            <entry name="MAV_CMD_FORCE_INIT" value="52">
                <description>Command to force the initialization even if there where errors.</description>
            </entry>
            <entry name="MAV_CMD_NOSECONE_OPEN" value="60">
                <description>Command to open the nosecone.</description>
            </entry>
            <entry name="MAV_CMD_DPL_WIGGLE_SERVO" value="61">
                <description>Command to wiggle the deployment servo-motor to check if it's working</description>
            </entry>
            <entry name="MAV_CMD_ARB_WIGGLE_SERVO" value="62">
                <description>Command to wiggle the aerobrake servo-motor to check if it's working</description>
            </entry>
            <entry name="MAV_CMD_DPL_RESET_SERVO" value="63">
                <description>Command to wiggle the deployment servo-motor to check if it's working</description>
            </entry>
            <entry name="MAV_CMD_ARB_RESET_SERVO" value="64">
                <description>Command to wiggle the aerobrake servo-motor to check if it's working</description>
            </entry>
            <entry name="MAV_CMD_CUT_DROGUE" value="65">
                <description>Command to activate the thermal cutters and cut the drogue, activating both thermal cutters sequentially</description>
            </entry>
            <entry name="MAV_CMD_START_LOGGING" value="70">
                <description>Command to enable sensor logging.</description>
            </entry>
            <entry name="MAV_CMD_CLOSE_LOG" value="72">
                <description>Command to permanently close the log file.</description>
            </entry>
            <entry name="MAV_CMD_TEST_AEROBRAKES" value="73">
                <description>Command to test the aerobrakes using a predefined servo sequence</description>
            </entry>
            <entry name="MAV_CMD_DISABLE_AEROBRAKES" value="74">
                <description>Disables aerobrakes during flight</description>
            </entry>
            <entry name="MAV_CMD_END_MISSION" value="100">
                <description>Command to communicate the end of the mission and close the file descriptors in the SD card.</description>
            </entry>
            <entry name="MAV_CMD_BOARD_RESET" value="201">
                <description>Command to reset the board from test status.</description>
            </entry>
            <entry name="MAV_CMD_TEST_MODE" value="200">
                <description>Command to enter the test mode.</description>
            </entry>
            <entry name="MAV_CMD_SERIAL_TM" value="202">
                <description>Command to enable mavlink messages over serial instead of radio.</description>
            </entry>
            <entry name="MAV_CMD_RADIO_TM" value="203">
                <description>Command to disabled mavlink messages over serial (start with radio).</description>
            </entry>
        </enum>
    </enums>
    <messages>
        <message id="1" msg_type="tc" name="PING_TC">
            <description>TC to ping the rocket (expects an ACK message as a response).</description>
            <field name="timestamp" type="uint64_t">Timestamp to identify when it was sent.</field>
        </message>
        <message id="10" msg_type="tc" name="NOARG_TC">
            <description>TC containing a command with no parameters.</description>
            <field name="command_id" type="uint8_t">A member of the MavCommandList enum.</field>
        </message>
        <message id="20" msg_type="tc" name="START_LAUNCH_TC">
            <description>TC to start the Launch sequence forwarding the launch_code to the ignition board.</description>
            <field name="launch_code" type="uint64_t">64bit launch code.</field>
        </message>
        <message id="21" msg_type="tc" name="UPLOAD_SETTING_TC">
            <description>TC to upload the value of a setting on the rocket.</description>
            <field name="setting_id" type="uint8_t">A member of the MavSettingList enum.</field>
            <field name="setting" type="float">The value of the setting to be uploaded</field>
        </message>
        <message id="22" msg_type="tc" name="SET_AEROBRAKE_ANGLE_TC">
            <description>Sets the extension angle of the aerobrakes</description>
            <field name="angle" type="float" units="deg">Aerobrake angle</field>
        </message>
        <message id="23" msg_type="tc" name="SET_REFERENCE_ALTITUDE">
            <description>Sets the reference altitude for the altimeter</description>
            <field name="ref_altitude" type="float" units="m">Reference altitude</field>
        </message>
        <message id="24" msg_type="tc" name="SET_REFERENCE_TEMPERATURE_TC">
            <description>Sets the reference temperature for the altimeter</description>
            <field name="ref_temp" type="float" units="degC">Reference temperature</field>
        </message>
        <message id="25" msg_type="tc" name="SET_DEPLOYMENT_ALTITUDE_TC">
            <description>Sets the deployment altitude for the main parachute</description>
            <field name="dpl_altitude" type="float" units="m">Deployment altitude</field>
        </message>
        <message id="26" msg_type="tc" name="SET_INITIAL_ORIENTATION_TC">
            <description>Sets the initial orientation for the navigation system</description>
            <field name="yaw" type="float" units="deg">Yaw angle</field>
            <field name="pitch" type="float" units="deg">Pitch angle</field>
            <field name="roll" type="float" units="deg">Roll angle</field>
        </message>
        <message id="51" msg_type="tc" name="SET_INITIAL_COORDINATES_TC">
            <description>Sets the initial launchpad coordinates</description>
            <field name="latitude" type="float" units="deg">Launchpad latitude</field>
            <field name="longitude" type="float" units="deg">Launchpad longitude</field>
        </message>
        <message id="50" msg_type="tc" name="TELEMETRY_REQUEST_TC">
            <description>TC containing a request for the status of a board.</description>
            <field name="board_id" type="uint8_t">A member of the MavTMList enum.</field>
        </message>
        <message id="80" msg_type="tc" name="RAW_EVENT_TC">
            <description>TC containing a raw event to be posted directly in the EventBroker (only for testing).</description>
            <field name="Event_id" type="uint8_t">Id of the event to be posted.</field>
            <field name="Topic_id" type="uint8_t">Id of the topic to which the event should be posted.</field>
        </message>
        <message id="130" msg_type="tm" name="ACK_TM">
            <description>TM containing an ACK message to notify that the message has been received.</description>
            <field name="recv_msgid" type="uint8_t">Message id of the received message.</field>
            <field name="seq_ack" type="uint8_t">Sequence number of the received message.</field>
        </message>
        <message id="131" msg_type="tm" name="NACK_TM">
            <description>TM containing a NACK message to notify that the received message was invalid.</description>
            <field name="recv_msgid" type="uint8_t">Message id of the received message.</field>
            <field name="seq_ack" type="uint8_t">Sequence number of the received message.</field>
        </message>
